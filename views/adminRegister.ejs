<title><%= title %></title>

<% include partials/navbar.ejs%>

<div class="container text-center">
    <h1 class="lead display-3"><%= pageTitle %></h1>
</div>

<!-- Work on messaging -->


<div class="container">

    <form action="/adminRegister" method="POST" id="adminRegister" class="form">

        </br>

        <div class="form-group fieldset contact">
            <label for=""><%= adminRegFirstName %></label>
            <input type="text" class="form-control mb-2"  placeholder="First name"   name="adminRegFirstName" required autofocus>
        </div>

        <div class="form-group fieldset contact">
            <label for=""><%= adminRegLastName %></label>
            <input type="text" class="form-control mb-2"  placeholder="Last name"  name="adminRegLastName" required>
        </div>

        <div class="form-group fieldset contact">
            <label for=""><%= adminRegUsername %></label>
            <input type="text" class="form-control mb-2"  placeholder="Username"  name="adminRegUsername" required>
        </div>

        <div class="form-group fieldset contact">
            <label for=""><%= adminRegPassword %></label>
            <input type="password" class="form-control mb-2"  placeholder="Password"   name="adminRegPassword" required>
        </div>

        </br>

        <div class="form-group fieldset">
            <button class="btn btn-primary" type="submit">Submit</button>
        </div>
    </form>
</div>

<script>
    $(document).ready( function() {
        $('.dropdown-toggle').dropdown();
